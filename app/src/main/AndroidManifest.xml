<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.siedg.acciontest">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".presentation.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_cat_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_cat_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AccionTest">
        <activity android:name="com.siedg.acciontest.presentation.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>